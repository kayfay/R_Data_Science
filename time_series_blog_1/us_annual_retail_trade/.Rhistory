op <- par(cex.main = 1.5, mar = c(5, 6, 4, 5) + 0.3, mgp = c(3.5, 1, 0), cex.lab = 1.5, font.lab = 2, cex.axis = 1.5, bty = "n", las = 1)
plot(forecast(arima.percap.tot, h=2), xlab = "", ylab = "", main = "2016 Predicted Total Retailer Sales", type="p", axes = FALSE)
axis(1) ; axis(2)
mtext("Thousands of Dollars", side = 1, line = 2.5, cex = 1.5)
mtext("Number of Years Since 1992", side = 2, line = 4.5, cex = 1.5, las = 0)
dev.off()
dir()
